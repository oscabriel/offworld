{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"globalEnv": ["CONVEX_URL", "WORKOS_CLIENT_ID"],
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": ["dist/**"]
		},
		"lint": {
			"dependsOn": ["^lint"]
		},
		"typecheck": {
			"dependsOn": ["^typecheck"]
		},
		"test": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", "src/**/*.test.ts"],
			"outputs": []
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"dev:setup": {
			"cache": false,
			"persistent": true
		},
		"deploy": {
			"cache": false
		},
		"destroy": {
			"cache": false
		},
		"clean:vite": {
			"cache": false
		}
	}
}
