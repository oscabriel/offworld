{
	"name": "@offworld/types",
	"version": "0.1.0",
	"license": "MIT",
	"files": [
		"dist"
	],
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.mts",
			"import": "./dist/index.mjs"
		},
		"./schemas": {
			"types": "./dist/schemas.d.mts",
			"import": "./dist/schemas.mjs"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "tsdown",
		"dev": "tsdown --watch",
		"typecheck": "tsc --noEmit",
		"test": "vitest run",
		"test:watch": "vitest",
		"prepublishOnly": "bun run build"
	},
	"dependencies": {
		"zod": "catalog:"
	},
	"devDependencies": {
		"tsdown": "catalog:",
		"typescript": "catalog:",
		"vitest": "catalog:"
	}
}
